<?xml version="1.0" encoding="UTF-8"?>
<aiml>

<category><pattern>lis-moi *</pattern>
<template>
<li>C'est une bonne idée, choisis une vidéo</li>
<think>
            <set name="PlayUtub"><star/></set>
         </think>
	<oob>
					<mrl>
				   <service>python</service>
				   <method>exec</method>
				   <param>PlayUtub("<star/>",0)</param>
				 </mrl>
			</oob>
</template>
</category> 

  
<category><pattern>Numéro *</pattern>
<template>
	<oob>
					<mrl>
				   <service>python</service>
				   <method>exec</method>
				   <param>PlayUtub("<get name="PlayUtub"/>",<star index="2" />)</param>
				 </mrl>
			</oob>
J'aurrais choisi la même
</template>
</category>

<category><pattern>VIDéO *</pattern>
<template><srai># Numéro <star index="2" /></srai></template>
</category>


<category><pattern>STOP</pattern><that># choisi la même #</that>
<template>
	<oob>
					<mrl>
				   <service>python</service>
				   <method>exec</method>
				   <param>PlayUtub("stop",0)</param>
				 </mrl>
			</oob>
</template>
</category>    

<category><pattern>REGARDER *</pattern>
<template><srai>LIS-MOI <star index="2" /></srai></template>
</category>


</aiml>

